    <script>
        const ctx = document.getElementById('skillsChart').getContext('2d');
        const skillsChart = new Chart(ctx, {
            type: 'bar',
            data: {
                labels: ['Leadership', 'Communication', 'Analytics', 'Execution', 'Strategy'],
                datasets: [{
                    label: 'Proficiency %',
                    data: [100, 95, 90, 95, 80],
                    backgroundColor: '#39A3F7',
                    borderColor: '#39A3F7',
                    borderWidth: 0,
                    borderRadius: 4,
                    barThickness: 20
                }]
            },
            options: {
                indexAxis: 'y',
                responsive: true,
                maintainAspectRatio: true,
                plugins: {
                    legend: {
                        display: false
                    },
                    tooltip: {
                        callbacks: {
                            label: function(context) {
                                return context.parsed.x + '%';
                            }
                        }
                    }
                },
                scales: {
                    x: {
                        beginAtZero: true,
                        max: 100,
                        ticks: {
                            font: {
                                size: 9
                            },
                            callback: function(value) {
                                return value + '%';
                            }
                        },
                        grid: {
                            color: 'rgba(0, 0, 0, 0.05)'
                        }
                    },
                    y: {
                        ticks: {
                            font: {
                                size: 10,
                                weight: 'bold'
                            }
                        },
                        grid: {
                            display: false
                        },
                        categoryPercentage: 0.4,
                        barPercentage: 1.0
                    }
                }
            }
        });
    </script>
