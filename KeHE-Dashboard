                            <script>
                                function toggleRetailers(element) {
                                    const arrow = element.querySelector('.retailer-arrow');
                                    const list = element.nextElementSibling;
                                    arrow.classList.toggle('open');
                                    list.classList.toggle('open');
                                }
                            </script>

